.google_image_element{ }
  - img_dom_id = dom_id(page_element,"image").gsub(/_/,'')
  %a{ :href => page_element.extra_data["dest_url"] || page_element.extra_data["image_url"], :target => "_blank" }
    %img{ :id => img_dom_id, :src => page_element.extra_data["image_url"], :width => page_element.width, :height => page_element.height }
  = render :partial => 'page_elements/_page_element_image_reload', :locals => { :page_element => page_element, :img_dom_id => img_dom_id }
  = render :partial => 'page_elements/_page_element_rotation', :locals => { :page_element => page_element }
